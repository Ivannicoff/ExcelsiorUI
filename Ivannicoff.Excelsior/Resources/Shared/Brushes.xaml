<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Text Default Brushes -->
    <SolidColorBrush x:Key="TextDefaultForegroundBrush" Color="{StaticResource TextFillPrimaryColor}" />
    <SolidColorBrush x:Key="TextSecondaryForegroundBrush" Color="{StaticResource TextFillSecondaryColor}" />
    <SolidColorBrush x:Key="TextTertiaryForegroundBrush" Color="{StaticResource TextFillTertiaryColor}" />
    <SolidColorBrush x:Key="TextSelectedForegroundBrush" Color="{StaticResource TextFillSelectedColor}" />
    <SolidColorBrush x:Key="TextDisabledForegroundBrush" Color="{StaticResource TextFillDisabledColor}" />

</ResourceDictionary>